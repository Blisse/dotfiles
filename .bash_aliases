# Parenting changing perms on / #
alias chown='chown --preserve-root'
alias chmod='chmod --preserve-root'
alias chgrp='chgrp --preserve-root'

# Folders
alias github='cd C:/Users/Victor/Documents/GitHub'
alias victor='cd C:/Users/Victor/'

# Ruby Fix
alias gem='C:/Ruby22-x64/bin/gem'
alias jekyll='C:/Ruby22-x64/bin/jekyll'
alias sass='C:/Ruby22-x64/bin/sass'
alias compass='C:/Ruby22-x64/bin/compass'

# Jekyll
alias jss='jekyll serve --watch --drafts && sass --watch assets/stylesheets/:assets/stylesheets/generated/'

alias run-ssh-agent='eval `/usr/bin/ssh-agent.exe -s`'
